{
  "name": "@quisido/worker-test",
  "version": "1.0.1",
  "author": "quisi.do <worker-test@quisi.do>",
  "description": "shared testing utilities for quisi.do Cloudflare workers modules",
  "homepage": "https://github.com/quisido/quisi.do/tree/main/packages/worker-test#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "bugs": {
    "email": "worker-test@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "scripts": {
    "attw": "attw --quiet",
    "clean": "rm -rf coverage dist node_modules && rm -f tsconfig.prepack.tsbuildinfo tsc-output.cpuprofile",
    "dev": "npm run tsc:watch",
    "eslint": "eslint . --cache --color --exit-on-fatal-error --max-warnings 0",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix --max-warnings 0",
    "prepack": "npm run tsc",
    "test": "concurrently --names attw,eslint,publint,vitest --prefix-colors auto \"npm run attw\" \"npm run eslint\" \"npm run publint\" \"npm run vitest:run\"",
    "publint": "publint",
    "tsc": "tsc --project tsconfig.prepack.json",
    "tsc:watch": "tsc --project tsconfig.prepack.json --watch",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:watch": "vitest watch"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250425.0",
    "@quisido/worker": "^1.0.0",
    "cloudflare-test-utils": "^1.0.0",
    "cloudflare-utils": "^1.0.0",
    "fmrs": "^2.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@microsoft/api-extractor": "^7.52.5",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@quisido/eslint-config": "^1.0.0",
    "@quisido/vitest-config": "^1.0.0",
    "@types/cookie": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitest/coverage-istanbul": "^3.1.2",
    "babel-plugin-replace-import-extension": "^1.1.5",
    "cjs-ts": "^1.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "vitest": ">=2.0.0"
  }
}
