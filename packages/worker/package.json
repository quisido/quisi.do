{
  "name": "@quisido/worker",
  "version": "1.0.8",
  "author": "quisi.do <worker@quisi.do>",
  "description": "shared quisi.do Cloudflare workers modules",
  "homepage": "https://github.com/quisido/quisi.do/tree/main/packages/worker#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "bugs": {
    "email": "worker@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "scripts": {
    "api-extractor": "node --experimental-strip-types scripts/api-extractor.ts",
    "attw": "attw --quiet",
    "build": "npm run tsc",
    "clean": "rimraf coverage dist node_modules tsc-output.cpuprofile tsconfig.build.tsbuildinfo",
    "dev": "npm run tsc:watch",
    "dev:dependencies": "node --experimental-strip-types scripts/dev.ts",
    "eslint": "eslint . --cache --color --exit-on-fatal-error --max-warnings 0",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix --max-warnings 0",
    "publint": "publint",
    "test": "concurrently --names attw,eslint,publint,vitest --prefix-colors auto \"npm run attw\" \"npm run eslint\" \"npm run publint\" \"npm run vitest:run\"",
    "tsc": "tsc --project tsconfig.build.json",
    "tsc:watch": "tsc --project tsconfig.build.json --watch",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:watch": "vitest watch"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250523.0",
    "cloudflare-utils": "^1.0.3",
    "cookie": "^1.0.2",
    "eventemitter3": "^5.0.1",
    "fmrs": "^2.0.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.1",
    "@microsoft/api-extractor": "^7.52.8",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@quisido/eslint-config": "^1.1.6",
    "@quisido/vitest-config": "^1.0.2",
    "@types/cookie": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-istanbul": "^3.1.4",
    "cjs-ts": "^2.0.0",
    "cloudflare-test-utils": "^1.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
