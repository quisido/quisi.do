{
  "name": "@quisido/dashboard",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf .wrangler coverage",
    "dev": "wrangler dev --persist-to ../../.wrangler/state",
    "eslint": "eslint . --cache --color --exit-on-fatal-error --max-warnings 0",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix --max-warnings 0",
    "login": "wrangler login",
    "prepack": "exit 0",
    "test": "concurrently --names eslint,publint,vitest --prefix-colors auto \"npm run-script eslint\" \"npm run-script publint\" \"npm run-script vitest:run\"",
    "production:deploy": "wrangler deploy src/index.ts --env production --outdir dist",
    "production:deploy:dry-run": "wrangler deploy src/index.ts --dry-run --env production --outdir dist",
    "publint": "publint",
    "staging:deploy": "wrangler deploy src/index.ts --env staging --outdir dist --remote",
    "staging:deploy:dry-run": "wrangler deploy src/index.ts --dry-run --env staging --outdir dist --remote",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:watch": "vitest watch",
    "whoami": "wrangler whoami"
  },
  "dependencies": {
    "@datadog/datadog-api-client": "^1.34.1",
    "@quisido/worker": "^1.0.0",
    "cloudflare-utils": "^1.0.0",
    "fmrs": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@cloudflare/workers-types": "^4.20250425.0",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@quisido/eslint-config": "^1.0.0",
    "@quisido/vitest-config": "^1.0.0",
    "@quisido/worker-test": "^1.0.0",
    "@types/eslint": "^9.6.1",
    "@vitest/coverage-istanbul": "^3.1.2",
    "babel-plugin-replace-import-extension": "^1.1.5",
    "cjs-ts": "^1.0.0",
    "cloudflare-test-utils": "^1.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "publint": "^0.3.12",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vitest": "^3.1.2",
    "wrangler": "^4.13.1"
  }
}
