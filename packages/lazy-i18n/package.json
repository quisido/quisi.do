{
  "name": "lazy-i18n",
  "version": "3.0.4",
  "author": "quisi.do <lazy-i18n@quisi.do>",
  "description": "Lazy loads translations for React apps",
  "homepage": "https://www.npmjs.com/package/lazy-i18n",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run tsc",
    "test": "concurrently --names eslint,publint,vitest --prefix-colors auto \"npm run eslint\" \"npm run publint\" \"npm run vitest:run\"",
    "attw": "attw --quiet",
    "clean": "rimraf coverage dist node_modules tsc-output.cpuprofile tsconfig.build.tsbuildinfo",
    "eslint": "eslint . --cache --color --exit-on-fatal-error",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix",
    "publint": "publint",
    "tsc": "tsc --project tsconfig.build.json",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:watch": "vitest watch"
  },
  "bugs": {
    "email": "lazy-i18n@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "dependencies": {
    "fmrs": "^2.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@quisido/eslint-config": "^1.1.8",
    "@quisido/vitest-config": "^1.0.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "cjs-ts": "^2.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.13",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-test-renderer": "^19.2.0",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
