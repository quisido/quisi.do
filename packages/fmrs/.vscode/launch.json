{
  "configurations": [
    {
      "console": "integratedTerminal",
      "name": "Build",
      "outFiles": ["${workspaceFolder}/dist/**/*"],
      "request": "launch",
      "runtimeArgs": ["run", "build"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "type": "node"
    },
    {
      "console": "integratedTerminal",
      "name": "Unit test",
      "outFiles": ["${workspaceFolder}/dist/**/*"],
      "request": "launch",
      "runtimeArgs": ["run", "vitest"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "type": "node"
    }
  ],
  "version": "0.2.0"
}
