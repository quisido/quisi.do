{
  "name": "cjs-ts",
  "version": "2.0.0",
  "author": "quisi.do <cjs-ts@quisi.do>",
  "description": "a Babel preconfiguration to bridge CommonJS and TypeScript files",
  "homepage": "https://github.com/quisido/quisi.do/tree/main/packages/cjs-ts#readme",
  "license": "MIT",
  "main": "./index.cjs",
  "type": "commonjs",
  "types": "./index.d.ts",
  "scripts": {
    "build": "exit 0",
    "test": "concurrently --names eslint,publint --prefix-colors auto \"npm run-script eslint\" \"npm run-script publint\"",
    "attw": "attw --quiet",
    "clean": "exit 0",
    "eslint": "eslint . --cache --color --exit-on-fatal-error",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix",
    "publint": "publint"
  },
  "bugs": {
    "email": "cjs-ts@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.cjs"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.28.3",
    "babel-plugin-replace-import-extension": "^1.1.5"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/babel__register": "^7.17.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "publint": "^0.3.13",
    "typescript": "^5.9.3"
  }
}
