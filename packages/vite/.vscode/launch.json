{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "npm run-script start",
      "request": "launch",
      "runtimeArgs": ["run-script", "start"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "localhost:3000",
      "request": "launch",
      "type": "chrome",
      "url": "https://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
