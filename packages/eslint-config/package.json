{
  "name": "@quisido/eslint-config",
  "version": "1.1.9",
  "author": "quisi.do <eslint-config@quisi.do>",
  "description": "ESLint configurations for quisi.do",
  "homepage": "https://github.com/quisido/quisi.do/tree/main/packages/eslint-config#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run-script tsc",
    "start": "tsc --project tsconfig.build.json --watch",
    "test": "concurrently --names eslint,publint,vitest --prefix-colors auto \"npm run-script eslint\" \"npm run-script publint\" \"npm run-script vitest:run\"",
    "attw": "attw --quiet",
    "clean": "rimraf coverage dist node_modules tsc-output.cpuprofile tsconfig.build.tsbuildinfo",
    "eslint": "eslint . --cache --color --exit-on-fatal-error",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix",
    "publint": "publint",
    "tsc": "tsc --project tsconfig.build.json",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:watch": "vitest watch"
  },
  "bugs": {
    "email": "eslint-config@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./cjs": {
      "types": "./dist/cjs.d.ts",
      "import": "./dist/cjs.js",
      "default": "./dist/cjs.js"
    },
    "./d-ts": {
      "types": "./dist/d-ts.d.ts",
      "import": "./dist/d-ts.js",
      "default": "./dist/d-ts.js"
    },
    "./js": {
      "types": "./dist/js.d.ts",
      "import": "./dist/js.js",
      "default": "./dist/js.js"
    },
    "./json": {
      "types": "./dist/json.d.ts",
      "import": "./dist/json.js",
      "default": "./dist/json.js"
    },
    "./jsonc": {
      "types": "./dist/jsonc.d.ts",
      "import": "./dist/jsonc.js",
      "default": "./dist/jsonc.js"
    },
    "./package-json": {
      "types": "./dist/package-json.d.ts",
      "import": "./dist/package-json.js",
      "default": "./dist/package-json.js"
    },
    "./test-ts": {
      "types": "./dist/test-ts.d.ts",
      "import": "./dist/test-ts.js",
      "default": "./dist/test-ts.js"
    },
    "./ts": {
      "types": "./dist/ts.d.ts",
      "import": "./dist/ts.js",
      "default": "./dist/ts.js"
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "dependencies": {
    "@eslint/config-helpers": "^0.4.0",
    "@eslint/js": "^9.36.0",
    "@eslint/json": "^0.13.2",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sort-keys-custom-order": "^2.2.1",
    "jsonc-eslint-parser": "^2.4.1",
    "typescript-eslint": "^8.45.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/node": "^22.15.21",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^4.0.6",
    "cjs-ts": "^2.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "publint": "^0.3.13",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "prettier": ">=3.0.0"
  }
}
