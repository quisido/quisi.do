{
  "name": "@quisido/vitest-config",
  "version": "1.0.3",
  "author": "quisi.do <vitest-config@quisi.do>",
  "description": "vitest configurations for quisi.do",
  "homepage": "https://github.com/quisido/quisi.do/tree/main/packages/vitest-config#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run-script tsc",
    "test": "concurrently --names attw,eslint,publint --prefix-colors auto \"npm run-script attw\" \"npm run-script eslint\" \"npm run-script publint\"",
    "attw": "attw --quiet",
    "clean": "rimraf coverage dist node_modules tsc-output.cpuprofile tsconfig.build.tsbuildinfo",
    "eslint": "eslint . --cache --color --exit-on-fatal-error",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix",
    "publint": "publint",
    "tsc": "tsc --project tsconfig.build.json",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:watch": "vitest watch"
  },
  "bugs": {
    "email": "vitest-config@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/node": "^24.6.2",
    "@vitest/coverage-istanbul": "^3.2.4",
    "cjs-ts": "^2.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "publint": "^0.3.13",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "vite": ">=6.0.0",
    "vitest": ">=3.0.0"
  }
}
