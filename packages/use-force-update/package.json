{
  "name": "use-force-update",
  "version": "2.0.0",
  "author": "quisi.do <use-force-update@quisi.do>",
  "description": "React hook to force your function component to re-render",
  "homepage": "https://github.com/quisido/quisi.do/tree/main/packages/use-force-update#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run-script tsc",
    "test": "concurrently --names attw,eslint,publint,vitest --prefix-colors auto \"npm run-script attw\" \"npm run-script eslint\" \"npm run-script publint\" \"npm run-script vitest:run\"",
    "attw": "attw --quiet",
    "clean": "rimraf coverage dist node_modules tsc-output.cpuprofile tsconfig.build.tsbuildinfo",
    "eslint": "eslint . --cache --color --exit-on-fatal-error",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix",
    "publint": "publint",
    "tsc": "tsc --project tsconfig.build.json",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:watch": "vitest watch"
  },
  "bugs": {
    "email": "use-force-update@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@quisido/eslint-config": "^1.1.8",
    "@quisido/vitest-config": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^4.0.6",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.13",
    "react": "^19.2.0",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "^4.0.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
