{
  "name": "@quisi/do",
  "version": "1.0.0",
  "author": "quisi.do <contact@quisi.do>",
  "description": "quisi.do",
  "homepage": "https://quisi.do/",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "bugs": {
    "email": "bugs@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "scripts": {
    "attw": "attw --exclude-entrypoints .",
    "authn": "npm run-script dev --workspace=packages/authn",
    "browserslist:update-db": "browserslist --update-db",
    "build": "npm run-script build --workspace=packages/next",
    "clean": "rm -rf coverage node_modules sarif && npm run-script clean --if-present --workspaces",
    "csp": "npm run-script dev --workspace=packages/csp",
    "dashboard": "npm run-script dev --workspace=packages/dashboard",
    "eslint": "eslint . --cache --color --exit-on-fatal-error --max-warnings 0",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix --max-warnings 0",
    "eslint:fix:workspaces": "npm run-script eslint:fix --workspaces",
    "fmrs": "npm run-script dev --workspace=packages/fmrs",
    "lighthouse": "npm run-script lighthouse --workspace=packages/next",
    "mtt:test": "monorepo-template-test",
    "postlighthouse": "npm run-script postlighthouse --workspace=packages/next",
    "prepack": "npm run-script prepack --workspaces",
    "publint": "publint",
    "publint:workspaces": "npm run-script publint --workspaces",
    "publish": "npm publish --workspaces",
    "react-datadog": "npm run-script dev --workspace=packages/react-datadog",
    "start": "concurrently --kill-others --kill-others-on-fail --names authn,csp,dashboard,fmrs,next,react-datadog,worker --prefix-colors auto --restart-after exponential --restart-tries 9999 \"npm run-script authn\" \"npm run-script csp\" \"npm run-script dashboard\" \"npm run-script fmrs\" \"npm run-script next\" \"npm run-script react-datadog\" \"npm run-script worker\"",
    "test": "concurrently --names eslint,test,publint,test,vitest --prefix-colors auto \"npm run-script eslint\" \"npm run-script test:workspaces\" \"npm run-script publint\" \"npm run-script mtt:test\" \"npm run-script vitest:run\"",
    "test:workspaces": "npm run-script test --workspaces",
    "up": "npm upgrade --include-workspace-root --workspaces \"@*/*\" \"*\" && npm install --legacy-peer-deps --save-dev --workspace=packages/next eslint-plugin-react-hooks@experimental && npm install --legacy-peer-deps --save --workspace=packages/next next@canary react@experimental react-dom@experimental",
    "vitest": "vitest",
    "vitest:run": "exit 0 || vitest run",
    "vitest:watch": "vitest watch",
    "worker": "npm run-script dev --workspace=packages/worker"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.25.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@monorepo-template/test": "^3.0.0",
    "@quisido/eslint-config": "^1.0.0",
    "@quisido/vitest-config": "^1.0.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitest/coverage-istanbul": "^3.1.2",
    "babel-plugin-replace-import-extension": "^1.1.5",
    "browserslist": "^4.24.4",
    "cjs-ts": "^1.0.0",
    "concurrently": "^9.1.2",
    "dd-trace": "^5.49.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vitest": "^3.1.2"
  }
}
