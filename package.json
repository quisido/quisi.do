{
  "name": "@quisi/do",
  "version": "1.0.0",
  "author": "quisi.do <contact@quisi.do>",
  "description": "quisi.do",
  "homepage": "https://quisi.do/",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run tsc:build && node ./dist/scripts/build.js",
    "start": "concurrently --kill-others --kill-others-on-fail --names \"authn           \",\"cloudflare-utils\",\"csp             \",\"dashboard       \",\"eslint-config   \",\"fmrs            \",\"vite            \",\"worker          \" --prefix-colors auto --restart-after exponential --restart-tries 9999 \"npm run authn\" \"npm run cloudflare-utils\" \"npm run csp\" \"npm run dashboard\" \"npm run eslint-config\" \"npm run fmrs\" \"npm run vite\" \"npm run worker\"",
    "test": "quisi test && npm run test:workspaces",
    "authn": "npm start --workspace=packages/authn",
    "browserslist:update-db": "browserslist --update-db",
    "clean": "rimraf coverage node_modules sarif && npm run clean --if-present --workspaces",
    "cloudflare-utils": "npm start --workspace=packages/cloudflare-utils",
    "csp": "npm start --workspace=packages/csp",
    "dashboard": "npm start --workspace=packages/dashboard",
    "eslint": "quisi eslint",
    "eslint-config": "npm start --workspace=packages/eslint-config",
    "fmrs": "npm start --workspace=packages/fmrs",
    "game": "npm start --workspace=packages/game",
    "lighthouse": "npm start --workspace=packages/vite",
    "postlighthouse": "npm start --workspace=packages/vite",
    "publint": "quisi publint",
    "publish": "npm run tsc:build && node ./dist/scripts/publish.js",
    "quisi": "npm start --workspace=packages/quisi",
    "react-datadog": "npm start --workspace=packages/react-datadog",
    "test:workspaces": "npm run tsc:build && node ./dist/scripts/test.js",
    "tsc:build": "tsc --outDir dist --project tsconfig.build.json --skipLibCheck",
    "update": "npm run tsc:build && node ./dist/scripts/update.js",
    "vite": "npm start --workspace=packages/vite",
    "vitest": "vitest run",
    "worker": "npm start --workspace=packages/worker"
  },
  "bugs": {
    "email": "bugs@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@hint/formatter-codeframe": "^3.1.36",
    "@hint/formatter-html": "^4.3.17",
    "@hint/formatter-json": "^3.1.36",
    "@hint/formatter-stylish": "^3.1.36",
    "@hint/formatter-summary": "^3.0.39",
    "@hint/hint-doctype": "^3.3.27",
    "@hint/hint-https-only": "^2.4.27",
    "@hint/hint-meta-theme-color": "^4.0.23",
    "@hint/hint-minified-js": "^2.4.27",
    "@hint/hint-no-broken-links": "^4.2.27",
    "@hint/hint-no-p3p": "^3.3.27",
    "@hint/hint-performance-budget": "^2.4.27",
    "@hint/parser-javascript": "^3.1.25",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@quisido/eslint-config": "^1.1.11",
    "@quisido/vitest-config": "^1.0.6",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.2.2",
    "@vitest/ui": "^4.0.18",
    "browserslist": "^4.28.1",
    "canvas": "^3.2.1",
    "cjs-ts": "^2.0.0",
    "concurrently": "^9.1.2",
    "hint": "^7.1.13",
    "publint": "^0.3.12",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "@hint/connector-puppeteer": {
      "puppeteer-core": ">=22.11.1"
    },
    "eslint-plugin-react-hooks": {
      "zod-validation-error": "^4.0.0"
    }
  }
}
