{
  "name": "@quisi/do",
  "version": "1.0.0",
  "author": "quisi.do <contact@quisi.do>",
  "description": "quisi.do",
  "homepage": "https://quisi.do/",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "bugs": {
    "email": "bugs@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "scripts": {
    "attw": "attw --exclude-entrypoints .",
    "authn": "npm run dev --workspace=packages/authn",
    "browserslist:update-db": "browserslist --update-db",
    "build": "npm run build --workspace=packages/vite",
    "clean": "rimraf coverage node_modules sarif && npm run clean --if-present --workspaces",
    "csp": "npm run dev --workspace=packages/csp",
    "dashboard": "npm run dev --workspace=packages/dashboard",
    "eslint": "eslint . --cache --color --exit-on-fatal-error --max-warnings 0",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix --max-warnings 0",
    "eslint:fix:workspaces": "npm run eslint:fix --workspaces",
    "fmrs": "npm run dev --workspace=packages/fmrs",
    "lighthouse": "npm run lighthouse --workspace=packages/vite",
    "mtt:test": "monorepo-template-test",
    "postlighthouse": "npm run postlighthouse --workspace=packages/vite",
    "prepack": "tsc --outDir dist --skipLibCheck --target ESNext ./scripts/prepack.ts && node ./dist/prepack.js",
    "publint": "publint",
    "publint:workspaces": "npm run publint --workspaces",
    "publish": "npm publish --workspaces",
    "react-datadog": "npm run dev --workspace=packages/react-datadog",
    "start": "concurrently --kill-others --kill-others-on-fail --names authn,csp,dashboard,fmrs,react-datadog,vite,worker --prefix-colors auto --restart-after exponential --restart-tries 9999 \"npm run authn\" \"npm run csp\" \"npm run dashboard\" \"npm run fmrs\" \"npm run react-datadog\" \"npm run vite\" \"npm run worker\"",
    "test": "concurrently --names eslint,publint,test,test-workspaces --prefix-colors auto \"npm run eslint\" \"npm run publint\" \"npm run mtt:test\" \"npm run test:workspaces\"",
    "test:workspaces": "npm run test --workspaces",
    "up": "npm upgrade --include-workspace-root --workspaces \"@*/*\" \"*\"",
    "vite": "npm run start --workspace=packages/vite",
    "worker": "npm run dev --workspace=packages/worker"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.25.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@monorepo-template/test": "^3.0.0",
    "@quisido/eslint-config": "^1.0.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "babel-plugin-replace-import-extension": "^1.1.5",
    "browserslist": "^4.24.4",
    "cjs-ts": "^1.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}
