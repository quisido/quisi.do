{
  "name": "@quisi/do",
  "version": "1.0.0",
  "author": "quisi.do <contact@quisi.do>",
  "description": "quisi.do",
  "homepage": "https://quisi.do/",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --outDir dist --skipLibCheck --target ESNext ./scripts/build.ts && node ./dist/build.js",
    "start": "concurrently --kill-others --kill-others-on-fail --names \"authn           \",\"cloudflare-utils\",\"csp             \",\"dashboard       \",\"eslint-config   \",\"fmrs            \",\"vite            \",\"worker          \" --prefix-colors auto --restart-after exponential --restart-tries 9999 \"npm run authn\" \"npm run cloudflare-utils\" \"npm run csp\" \"npm run dashboard\" \"npm run eslint-config\" \"npm run fmrs\" \"npm run vite\" \"npm run worker\"",
    "test": "concurrently --names eslint,publint --prefix-colors auto \"npm run eslint\" \"npm run publint\"",
    "attw": "attw --exclude-entrypoints .",
    "authn": "npm run start --workspace=packages/authn",
    "browserslist:update-db": "browserslist --update-db",
    "clean": "rimraf coverage node_modules sarif && npm run clean --if-present --workspaces",
    "cloudflare-utils": "npm run start --workspace=packages/cloudflare-utils",
    "csp": "npm run start --workspace=packages/csp",
    "dashboard": "npm run start --workspace=packages/dashboard",
    "eslint": "eslint . --cache --color --exit-on-fatal-error",
    "eslint-config": "npm run start --workspace=packages/eslint-config",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix",
    "fmrs": "npm run start --workspace=packages/fmrs",
    "lighthouse": "npm run lighthouse --workspace=packages/vite",
    "postlighthouse": "npm run postlighthouse --workspace=packages/vite",
    "publint": "publint",
    "publish": "tsc --outDir dist --skipLibCheck --target ESNext ./scripts/publish.ts && node ./dist/publish.js",
    "react-datadog": "npm run start --workspace=packages/react-datadog",
    "update": "tsc --outDir dist --skipLibCheck --target ESNext ./scripts/update.ts && node ./dist/update.js",
    "vite": "npm run start --workspace=packages/vite",
    "worker": "npm run start --workspace=packages/worker"
  },
  "bugs": {
    "email": "bugs@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quisido/quisi.do.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@hint/formatter-codeframe": "^3.1.36",
    "@hint/formatter-html": "^4.3.17",
    "@hint/formatter-json": "^3.1.36",
    "@hint/formatter-stylish": "^3.1.36",
    "@hint/formatter-summary": "^3.0.39",
    "@hint/hint-doctype": "^3.3.27",
    "@hint/hint-https-only": "^2.4.27",
    "@hint/hint-meta-theme-color": "^4.0.23",
    "@hint/hint-minified-js": "^2.4.27",
    "@hint/hint-no-broken-links": "^4.2.27",
    "@hint/hint-no-p3p": "^3.3.27",
    "@hint/hint-performance-budget": "^2.4.27",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@quisido/eslint-config": "^1.1.8",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.21",
    "browserslist": "^4.24.5",
    "canvas": "^3.1.2",
    "cjs-ts": "^2.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "hint": "^7.1.13",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}
