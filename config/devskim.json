{
  "$schema": "../schemas/devskim.json",
  "Confidences": ["High", "Medium"],
  "CrawlArchives": false,
  "DisableParallel": false,
  "Globs": [".cache/", ".git/", ".tests/", ".wrangler/", "dist/", "node_modules/"],
  "IgnoreDefaultRules": false,
  "IgnoreRuleIds": [],
  "OutputFile": ".tests/devskim.sarif",
  "OutputFileFormat": "sarif",
  "Path": "./",
  "RespectGitIgnore": true,
  "Severities": ["BestPractice", "Critical", "Important", "ManualReview", "Moderate"],
  "SkipExcerpts": true
}
